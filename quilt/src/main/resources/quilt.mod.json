{
  "schema_version": 1,
  "mixin": [
    "projectiles.mixins.json",
    "projectiles-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "projectiles",
    "version": "${version}",
    "metadata": {
      "name": "Projectiles (Quilt)",
      "description": "",
      "contributors": {

      },
      "contact": {
        "sources": "https://github.com/creep3rcrafter/Mystic-Potions"
      },
      "icon": "assets/projectiles/logo.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.creep3rcrafter.projectiles.quilt.ProjectilesQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "<=${quiltLoaderVersion}"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=${minecraftVersion}"
      },
      {
        "id": "architectury",
        "version": ">=${architecturyVersion}"
      }
    ]
  }
}